{
  "mcpProxy": {
    "baseURL": "http://localhost:9090",
    "addr": ":9090",
    "name": "MCP Proxy",
    "version": "1.0.0",
    "options": {
      "panicIfInvalid": false,
      "logEnabled": false,
      "authTokens": [
        "AdminToken"
      ]
    }
  },
  "mcpServers": {
    "fetch": {
      "command": "npx",
      "args": [
        "-y",
        "fetch-mcp"
      ],
      "env": {
      },
      "options": {
        "panicIfInvalid": true,
        "logEnabled": true,
        "authTokens": [
          "HelloWorld"
        ]
      }
    },
    "exampleServer": {
      "url": "https://example.com/mcp-sse",
      "headers":  {
        "Authorization": "Bearer example-token"
      }
    }
  }
}